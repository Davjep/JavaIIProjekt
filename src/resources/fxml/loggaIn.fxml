<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoggaInController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="loggaInKnapp" layoutX="221.0" layoutY="300.0" mnemonicParsing="false" onAction="#loggaInKnappTryck" prefHeight="40.0" prefWidth="159.0" text="Logga in" />
                <TextField fx:id="emailFält" layoutX="222.0" layoutY="150.0" onAction="#emailInput" promptText="Skriv in email..." />
                <PasswordField fx:id="lösenordFält" layoutX="222.0" layoutY="200.0" onAction="#lösenordInput" promptText="Skriv in lösenord...." />
            <Label fx:id="användarnamnText" layoutX="222.0" layoutY="133.0" text="Email" />
            <Label fx:id="lösenordText" layoutX="222.0" layoutY="183.0" text="Lösenord" />
            <Label fx:id="errorText" alignment="CENTER" layoutX="2.0" layoutY="259.0" prefHeight="25.0" prefWidth="590.0" textAlignment="CENTER" textFill="RED" />
            <Label fx:id="loggaInText" alignment="CENTER" layoutX="236.0" layoutY="51.0" text="Logga In" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Button fx:id="anställdKnapp" layoutX="527.0" layoutY="14.0" mnemonicParsing="false" onAction="#anställdKnappTryck" text="Anställd" />
            </children>
        </AnchorPane>
    </children>
</VBox>
